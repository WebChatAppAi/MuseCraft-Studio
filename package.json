{
  "displayName": "MuseCraft",
  "name": "musecraft-app",
  "description": "AI MIDI Generation Studio",
  "version": "1.0.0",
  "main": "./node_modules/.dev/main/index.js",
  "resources": "src/resources",
  "author": { "name": "Alvan", "email": "jonasmikkelmind@gmail.com" },
  "license": "PVT",
  "packageManager": "pnpm@10.0.0",
  "engines": { "node": ">=18 <=22" },
  "scripts": {
    "start": "electron-vite preview",
    "predev": "run-s clean:dev compile:app",
    "dev": "cross-env NODE_ENV=development electron-vite dev --watch",
    "compile:app": "electron-vite build",
    "compile:packageJSON": "tsx ./src/lib/electron-app/release/modules/prebuild.ts",
    "prebuild": "run-s clean:dev compile:app compile:packageJSON",
    "build": "pnpm electron-builder",
    "build:win": "pnpm run prebuild && pnpm electron-builder --win",
    "build:mac": "pnpm run prebuild && pnpm electron-builder --mac", 
    "build:linux": "pnpm run prebuild && pnpm electron-builder --linux",
    "build:all": "pnpm run prebuild && pnpm electron-builder -mwl",
    "make:release": "tsx ./src/lib/electron-app/release/modules/release.ts",
    "release": "electron-builder --publish always",
    "clean:dev": "rimraf ./node_modules/.dev",
    "clean:dist": "rimraf ./dist",
    "clean": "run-s clean:dev clean:dist",
    "lint": "biome lint --no-errors-on-unmatched",
    "lint:fix": "biome lint --write --no-errors-on-unmatched",
    "postinstall": "node -e \"console.log('🔧 Running post-install checks...'); try { require('electron'); console.log('✅ Electron is ready'); } catch (e) { console.log('⚠️ Reinstalling Electron...'); require('child_process').execSync('pnpm add electron --save-dev --force', {stdio: 'inherit'}); console.log('✅ Electron reinstalled'); }\" && (electron-builder install-app-deps || echo 'Warning: electron-builder install-app-deps failed but continuing...')"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tonejs/piano": "^0.2.1",
    "buffer": "^6.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "crypto-browserify": "^3.12.1",
    "crypto-js": "^4.2.0",
    "electron-router-dom": "^2.1.0",
    "events": "^3.3.0",
    "framer-motion": "^12.0.0",
    "lucide-react": "^0.477.0",
    "midi-writer-js": "^3.1.1",
    "process": "^0.11.10",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.2.0",
    "sf2-synth-audio-worklet": "^0.0.12",
    "soundfont-player": "^0.12.0",
    "stream-browserify": "^3.0.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "tone": "^15.1.22",
    "util": "^0.12.5",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@tailwindcss/vite": "^4.0.9",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^22.13.8",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.0.4",
    "code-inspector-plugin": "^1.0.4",
    "cross-env": "^7.0.3",
    "electron": "^37.3.1",
    "electron-builder": "^25.1.8",
    "electron-extension-installer": "^2.0.0",
    "electron-vite": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "rollup-plugin-inject-process-env": "^1.3.1",
    "tailwindcss": "^4.1.12",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": ["@tailwindcss/oxide", "electron", "esbuild", "midi-writer-js"],
    "onlyBuiltDependenciesFile": "only-built-dependencies.json"
  }
}
